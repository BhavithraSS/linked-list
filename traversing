class Node:
    def __init__(self,d):
                 self.data=d
                 self.next=None
class Linkedlist:
    def __init__(self):
        self.head=None
    def listprint(self):
        printval=self.head
list1=Linkedlist()
list1.head=Node("mon")
e2=Node("tue")
e3=Node("wed")
list1.head.next=e2
list1.next=e3
list1.listprint()
temp=list1.head
while temp:
    print(temp.data,"=>",end='')
    temp=temp.next
